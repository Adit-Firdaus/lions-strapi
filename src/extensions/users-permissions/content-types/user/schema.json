{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "name": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "district": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::district.district",
      "inversedBy": "users"
    },
    "club": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::club.club",
      "inversedBy": "users"
    },
    "member": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member",
      "mappedBy": "user"
    },
    "awards": {
      "type": "component",
      "repeatable": true,
      "component": "user.award"
    },
    "achivements": {
      "type": "component",
      "repeatable": true,
      "component": "user.image"
    },
    "social": {
      "type": "component",
      "repeatable": true,
      "component": "user.social"
    },
    "trainings": {
      "type": "component",
      "repeatable": true,
      "component": "user.training"
    },
    "memberID": {
      "type": "string"
    },
    "about": {
      "type": "text"
    },
    "phone": {
      "type": "component",
      "repeatable": false,
      "component": "user.social"
    }
  }
}
